@page
@model lab1_Cafe.Pages.ReportsModel
@{
    ViewData["Title"] = "Reports";
}
<div class="text-left">
    <h2>Сформировать отчёт</h2>
    <form method="post" class="w-25">
        <div class="form-group">
            <label>С: </label>
            <input type="datetime-local" class="form-control" name="startTime" required/>
        </div>
        <div class="form-group">
            <label>По: </label>
            <input type="datetime-local" class="form-control" name="endTime" required/>
        </div>
        <button class="btn btn-secondary" type="submit">Сформировать</button>
    </form>
</div>